{
  "name": "nitsan/ns-migration",
  "type": "typo3-cms-extension",
  "description": "Explore https://t3terminal.com and https://nitsantech.com",
  "keywords": [
    "TYPO3 CMS",
    "migration",
    "ns"
  ],
  "authors": [
    {
        "name": "Nilesh Malankiya",
        "email": "nilesh@nitsantech.com",
        "role": "Developer"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "NITSAN\\NsMigration\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/ns-migration": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
      "typo3/cms": {
          "cms-package-dir": "{$vendor-dir}/typo3/cms",
          "web-dir": ".Build/public",
          "extension-key": "ns_migration"
      }
  }
}